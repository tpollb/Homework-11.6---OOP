            <ComboBoxItem IsSelected="True">Consultant</ComboBoxItem>
        </ComboBox>
        <Label Content="Login" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Selec_User_Button" Content="Select" HorizontalAlignment="Center" Margin="0,75,0,0" VerticalAlignment="Top" Width="104" Click="Selec_User_Button_Click"/>
    </Grid>
</Window>
