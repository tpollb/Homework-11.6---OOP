<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework_11._6___OOP"
        xmlns:SqlClient="clr-namespace:System.Data.SqlClient;assembly=System.Data" x:Class="Homework_11._6___OOP.MainWindow"
        mc:Ignorable="d"
        Title="Select user" Height="164" Width="265" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="user_list_ComboBox" HorizontalAlignment="Center" Margin="0,41,0,0" VerticalAlignment="Top" Width="214" IsReadOnly="True">
